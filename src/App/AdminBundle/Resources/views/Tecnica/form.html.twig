{% block body %}
	<form class="blogger" method="post" >
	{{ form_start(form) }}
	<div class="form-group">
		{{ form_row(form.nombre) }}
	</div>
	<div class="form-group">
		{{ form_row(form.fechaCreacion) }}
	</div>
	<div class="form-group">
		{{ form_row(form.descripcion) }}
	</div>
	<div class="form-group">
		{{ form_row(form.vid) }}
	</div>
	<div class="form-group">
		{{ form_row(form.utensilios) }}
	</div>

        {{ form_errors(form) }}
		
	{{ form_end(form) }}
	</form>

{% endblock %}
