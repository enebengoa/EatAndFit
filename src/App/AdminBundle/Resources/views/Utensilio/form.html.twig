{% block body %}
	{{ form_start(form) }}
	<div class="form-group">
		{{ form_row(form.nombre) }}
	</div>
	<div class="form-group">
		{{ form_row(form.descripcion) }}
	</div>
	<div class="form-group">
		{{ form_row(form.rating) }}
	</div>
	<div class="form-group">
		{{ form_row(form.imagen) }}
	</div>
        {{ form_errors(form) }}
	{{ form_end(form) }}
{% endblock %}
